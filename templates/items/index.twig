{% extends "layouts/base.twig" %}

{% block stylesheets %}
    <link rel="stylesheet" href="/css/items/items.css">
{% endblock %}

{% set myEntryQuery = craft.entries().section('items') %}
{% set entries = myEntryQuery.all() %}

{% block body %}
<div class="container">
    <h1>Items</h1>

    <ul>
        {% for entry in entries %}
            <li>
                <a href="{{ entry.url }}">{{ entry.title }}</a>
            </li>
        {% endfor %}
    </ul>
</div>
{% endblock %}


